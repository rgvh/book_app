<!DOCTYPE html>
<html lang="en">
  <head>
      <%- include('../layouts/head') %>
  </head>
  <body>
    <header>
        <%- include('../layouts/header') %>
    </header>
    <main>
      <h4>Search Results</h4>
      <div>
      <% searchResults.forEach(results => { %>
        <img src=<%= results.image_url %>>
      <h3><%= results.title %></h3>
      <ul>
        <li><%= results.author %></li>
      </ul>
      <p><%= results.description %></p>
    <% }); %>
      </div>
    </main>
  </body>
</html>