<div class="book-container">
  <img src=<%= book.image_url %>
  <section>
    <h3><%= book.title %>
      <span><%= book.author %></span>
    </h3>
    <p class="description"><%= book.description %></p>
    <ul>
      <li>ISBN: <%= book.isbn %></li>
      <li>Bookshelf:<%= book.bookshelf %></li>
    </ul>
  </section>
</div>