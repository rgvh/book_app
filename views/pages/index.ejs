<!DOCTYPE html>
<html lang="en">
  <head>
      <%- include('./layouts/head') %>
  </head>
  <body>
    <header>
        <%- include('../layouts/header') %>
    </header>
    <main>
      <h4>Search Results</h4>
        <ul class="search-results"></ul>
          <% getBooks.forEach(id => { %>
            <li data-book-id= <%= results.isbn %>
              <div class="book-container">
                <img src=<%= results.image_url %>
                <section>
                  <h3><%= results.title %></h3>
                  <span><%= results.author %></span>
                  <p class="desc"><%= results.description %></p>
                </section>
              </section>
            </li>
          <% }); %>
          </ul>
    </main>
  <footer>
</html>

